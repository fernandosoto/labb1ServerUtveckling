 xsi:schemaLocation="http://java.sun.com/xml/ns/persistence
http://java.sun.com/xml/ns/persistence/persistence_1_0.xsd"
 version="1.0">
 
<persistence-unit name="Labb1PU" transaction-type="RESOURCE_LOCAL">
 <provider>org.hibernate.ejb.HibernatePersistence</provider>
 <class>bo.User</class>
 <exclude-unlisted-classes>false</exclude-unlisted-classes>
 <properties>
 <property name="hibernate.connection.username" value="reine"/>
 <property name="hibernate.connection.driver_class" value="com.mysql.jdbc.Driver"/>
 <property name="hibernate.connection.password" value="forzabajen"/>
 <property name="hibernate.connection.url" value="jdbc:mysql://localhost:3306/reine"/>
 <property name="hibernate.cache.provider_class" value="org.hibernate.cache.NoCacheProvider"
/>
 </properties>
 </persistence-unit>
</persistence>